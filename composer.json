{
    "name": "nixox/nixox",
    "type": "library",
    "description": "Nixox lib for PHP 5.6",
    "keywords": ["nix","nixon", "psr-4"],
    "homepage": "https://github.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Bartosz Zwierzchowski",
            "email": "vedget.mbox@gmail.com",
            "homepage": "http://github.net/",
            "role": "Developer"
        }
    ],
	"require": {
        "php": "^7.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
		"php": ">=5.6.0",
		"phpunit/phpunit": "^5.3"
	}
    "autoload": {
        "psr-4": { "Nixox\\": "src/Nixox" }
    }
	"autoload-dev": {
        "psr-4": {"Nixox\\": "tests/Nixox"}
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
	}
}